{
  "swagger": "2.0",
  "info": {
    "description": "一个基于Swagger的API文档",
    "version": "0.0.1",
    "title": "Swagger文档",
    "contact": {
      "email": "1215627787@qq.com"
    }
  },
  "host": "localhost:3000",
  "basePath": "/",
  "tags": [
    {
      "name": "users",
      "description": "用户模块"
    }
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/users/add": {
      "post": {
        "tags": [
          "users"
        ],
        "summary": "新增一条用户数据",
        "description": "新增一条用户数据",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "required": ["email", "password"],
        "parameters": [
          {
            "name": "email",
            "in": "formData",
            "description": "用户邮箱",
            "type": "string"
          },
          {
            "name": "password",
            "in": "formData",
            "description": "用户密码",
            "type": "string"
          },
          {
            "name": "phone",
            "in": "formData",
            "description": "用户手机",
            "type": "string"
          },
          {
            "name": "active",
            "in": "formData",
            "description": "用户状态",
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "成功返回",
            "schema": {
              "type": "Object",
              "$ref": "#/definitions/ApiResponse"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "ApiResponse": {
      "type": "object",
      "properties": {
        "code": {
          "description": "返回码"
        },
        "msg": {
          "description": "提示信息"
        },
        "data": {
          "properties": {
            "a": {
              "description": "aaaaaaaaaa"
            }
          }
        }
      }
    }
  }
}